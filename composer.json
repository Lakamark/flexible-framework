{
    "name": "lakamark/flexible-framework",
    "description": "A PHP framework with a module system",
    "version": "1.0.0",
    "keywords": ["framework", "PHP"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc-Antoine Lacasse",
            "email": "m.a.l.musique@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "FlexibleFramework\\": "src/FlexibleFramework",
            "App\\": "src",
            "Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.3",
        "guzzlehttp/guzzle": "8.0.x-dev",
        "http-interop/response-sender": "dev-master",
        "mezzio/mezzio-fastroute": "3.14.x-dev",
        "twig/twig": "4.x-dev",
        "php-di/php-di": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.2@dev",
        "friendsofphp/php-cs-fixer": "dev-master",
        "symfony/var-dumper": "7.4.x-dev",
        "robmorgan/phinx": "0.x-dev",
        "fakerphp/faker": "2.0.x-dev"
    }
}
